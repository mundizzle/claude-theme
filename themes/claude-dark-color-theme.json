  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#78716C",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "string.quoted", "string.template"],
      "settings": {
        "foreground": "#9CAF9C"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.character"],
      "settings": {
        "foreground": "#FBBF24"
      }
    },
    {
      "scope": ["constant.other", "constant.other.color"],
      "settings": {
        "foreground": "#D97757"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#D97757"
      }
    },
    {
      "scope": ["keyword.control", "keyword.operator"],
      "settings": {
        "foreground": "#D97757"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#E9A23B"
      }
    },
    {
      "scope": ["entity.name.class", "entity.name.type", "support.class", "support.type"],
      "settings": {
        "foreground": "#5EADA4"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#D97757"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#FBBF24"
      }
    },
    {
      "scope": ["variable", "variable.other", "variable.parameter"],
      "settings": {
        "foreground": "#A78BFA"
      }
    },
    {
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#D97757",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["support.constant", "support.variable"],
      "settings": {
        "foreground": "#5EADA4"
      }
    },
    {
      "scope": ["punctuation", "meta.brace"],
      "settings": {
        "foreground": "#A8A29E"
      }
    },
    {
      "scope": ["meta.function-call"],
      "settings": {
        "foreground": "#E9A23B"
      }
    },
    {
      "scope": ["entity.name.namespace", "entity.name.module"],
      "settings": {
        "foreground": "#5EADA4"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#D97757",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#9CAF9C"
      }
    },
    {
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#78716C",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.list"],
      "settings": {
        "foreground": "#FBBF24"
      }
    },
    {
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#E9A23B"
      }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#EF4444"
      }
    },
    {
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#FBBF24",
        "fontStyle": "strikethrough"
      }
    }
  ]
}
