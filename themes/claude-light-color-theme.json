    "widget.shadow": "#00000011",
    "selection.background": "#D9775744",
    "descriptionForeground": "#78716C",
    "errorForeground": "#DC2626",

    "gitDecoration.modifiedResourceForeground": "#D97706",
    "gitDecoration.deletedResourceForeground": "#DC2626",
    "gitDecoration.untrackedResourceForeground": "#5A7A5A",
    "gitDecoration.ignoredResourceForeground": "#A8A29E",
    "gitDecoration.conflictingResourceForeground": "#D97757",

    "editorError.foreground": "#DC2626",
    "editorWarning.foreground": "#D97706",
    "editorInfo.foreground": "#B8860B",

    "minimap.selectionHighlight": "#D9775744",
    "minimap.findMatchHighlight": "#D97757",

    "peekView.border": "#D97757",
    "peekViewTitle.background": "#FAF7F4",
    "peekViewResult.background": "#FAF7F4",
    "peekViewEditor.background": "#FFFCF9"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#78716C",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "string.quoted", "string.template"],
      "settings": {
        "foreground": "#6B7D6B"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.character"],
      "settings": {
        "foreground": "#D97706"
      }
    },
    {
      "scope": ["constant.other", "constant.other.color"],
      "settings": {
        "foreground": "#D97757"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#D97757"
      }
    },
    {
      "scope": ["keyword.control", "keyword.operator"],
      "settings": {
        "foreground": "#D97757"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#B8860B"
      }
    },
    {
      "scope": ["entity.name.class", "entity.name.type", "support.class", "support.type"],
      "settings": {
        "foreground": "#3D8B80"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#D97757"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#D97706"
      }
    },
    {
      "scope": ["variable", "variable.other", "variable.parameter"],
      "settings": {
        "foreground": "#7C3AED"
      }
    },
    {
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#D97757",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["support.constant", "support.variable"],
      "settings": {
        "foreground": "#3D8B80"
      }
    },
    {
      "scope": ["punctuation", "meta.brace"],
      "settings": {
        "foreground": "#57534E"
      }
    },
    {
      "scope": ["meta.function-call"],
      "settings": {
        "foreground": "#B8860B"
      }
    },
    {
      "scope": ["entity.name.namespace", "entity.name.module"],
      "settings": {
        "foreground": "#3D8B80"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#D97757",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#6B7D6B"
      }
    },
    {
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#78716C",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.list"],
      "settings": {
        "foreground": "#D97706"
      }
    },
    {
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#B8860B"
      }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#DC2626"
      }
    },
    {
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#D97706",
        "fontStyle": "strikethrough"
      }
    }
  ]
}
