    "list.focusOutline": "#D97757",
    "list.focusAndSelectionOutline": "#D97757",
    "list.hoverBackground": "#F5F1ED",
    "list.inactiveSelectionBackground": "#D9775722",
    "list.highlightForeground": "#D97757",

    "statusBar.background": "#F5F1ED",
    "statusBar.foreground": "#57534E",
    "statusBar.debuggingBackground": "#D97757",
    "statusBar.debuggingForeground": "#FFFFFF",
